package step02;

public class Ex03Object {

	public static void main(String[] args) {
	
		A a = new A();
		B b = new B();
//		C c = new C();
		
	}

}

class A {
	A() {
		System.out.println("Class A");
	}
}

class B {
	A a =  new A();
	B() {
		System.out.println("Class B");
	}
}

class C {
	C() {
		System.out.println("Class C");
	}
}


